{
    "manifest_version": 3,
    "name": "Digihelper",
    "version": "1.0",
    "description": "A digital helper",
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "src/background/background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://grants.myrosmol.ru"],
            "css": ["src/content/styles.css"],
            "js": ["src/lib/react.development.js", "src/lib/react-dom.development.js", "src/content/Form.js", "src/content/App.js"]
        }
    ]
}
